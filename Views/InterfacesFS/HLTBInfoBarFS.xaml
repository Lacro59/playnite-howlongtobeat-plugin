             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:HowLongToBeat.Views.InterfacesFS"
             mc:Ignorable="d">

    <TextBlock Style="{DynamicResource TextBlockBaseStyle}"
               FontFamily="{DynamicResource FontTitilliumWebBold}"
               Text="{DynamicResource LOCHowLongToBeatTimeToBeat}"
               Margin="0,0,0,10" />
    <Grid>
        <TextBlock Style="{DynamicResource TextBlockBaseStyle}" Grid.Column="0" Text="..." Name="PART_HltbTime">
        </TextBlock>
    </Grid>
</StackPanel>
    